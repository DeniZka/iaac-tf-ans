[proxy]
bms-devops.ru ansible_connection=ssh ansible_user=root

[nodes]
#nginx
172.16.0.100 ansible_connection=ssh ansible_user=root

[nodes:vars]
#skip yes on host checking and move it to null
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -o ProxyCommand="ssh -p 22 -W %h:%p -q root@bms-devops.ru"'
